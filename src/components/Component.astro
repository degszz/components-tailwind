---
import { type CollectionEntry } from "astro:content";
type Props = CollectionEntry<"comps">["data"] & {slug:string};
const { image, title, slug } = Astro.props;
---

<a href={`/comps/${slug}`}>
    <div>
        <img src={image} alt="">
    </div>
    <div>
        <h3>{title}</h3>
    </div>
</a>